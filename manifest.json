{
  "manifest_version": 3,
  "name": "SnapLayer",
  "version": "0.1.0",
  "description": "",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "src/background.ts"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.ts"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
