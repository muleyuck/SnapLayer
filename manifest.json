{
  "manifest_version": 3,
  "name": "SnapLayer",
  "version": "0.1.0",
  "description": "lets you overlay JPEG/PNG/SVG images on any webpage.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.ts"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "favicon48.png",
    "128": "favicon128.png"
  }
}
